{
  "name": "lso",
  "version": "0.2.10",
  "manifest_version": 2,
  "description": "Spy",
  "icons": {
    "128": "icons/icon128.png"
  },




  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
     "default_popup": "popup.html",
     "default_title": "tool"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "notifications",
        "activeTab"

  ]
}

{
  "name": "HTTP-Headers",
  "version": "0.4.1",
  "manifest_version": 2,
  "description": "HTTP-Headers",
  "icons": {
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
     "default_popup": "popup.html",
     "default_title": "HTTP-Headers"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "notifications",
    "activeTab",
    "storage"

  ],
  "web_accessible_resources": [ "options.html",    "/js/options.js"]
}

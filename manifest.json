{
  "name": "HTTP-Headers K1",
  "version": "0.8.1",
  "manifest_version": 3,
  "description": "HTTP-Headers (Update in progress!)",
  "icons": {
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "background": {
     "service_worker":"js/background/module/background.js",
     "type": "module"
  },
  "action": {
     "default_popup": "popup.html",
     "default_title": "HTTP-Headers K1"
  },
    "content_scripts": [ {
        "js":        [ "/js/background/browser-fingerprint/javascript.js" ],
        "matches":   [ "<all_urls>"],
        "run_at":    "document_start"
    } ],
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "storage",
    "notifications",
    "webRequest",
    "browsingData"
  ],
  "web_accessible_resources": [{
    "resources": [ "options.html", "/js/options.js"],
    "matches": ["<all_urls>"]
  }]
}
